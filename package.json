{
  "name": "@hybsearch/hybsearch",
  "private": true,
  "version": "4.0.0-rc.2",
  "description": "Find nonmonophyly in genetic sequences",
  "repository": "hybsearch/hybsearch",
  "homepage": "https://hybsearch.github.io/",
  "main": "main.js",
  "license": "MIT",
  "author": "The Hybsearch Team <hybsearch@stolaf.edu>",
  "scripts": {
    "start": "cd electron && npm start",
    "test": "jest",
    "lint": "eslint --cache \"server/**/*.js\" \"server/**/hyb-*\" \"electron/**/*.js\"",
    "pretty-check": "npm run prettify -- --list-different",
    "p": "pretty-quick",
    "prettify": "prettier --write \"server/**/*.js\" \"server/**/hyb-*\" \"electron/**/*.{js,css}\""
  },
  "prettier": {
    "overrides": [
      {
        "files": "server/**/hyb-*",
        "options": {
          "parser": "babylon"
        }
      }
    ],
    "useTabs": true,
    "semi": false,
    "trailingComma": "es5",
    "singleQuote": true
  },
  "jest": {
    "verbose": true
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-plugin-import": "^2.12.0",
    "expect": "^22.4.3",
    "jest": "^22.4.4",
    "jest-specific-snapshot": "^0.5.0",
    "parcel-bundler": "^1.8.1",
    "prettier": "^1.12.1",
    "pretty-quick": "^1.5.1"
  }
}
