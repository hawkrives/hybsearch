{
  "name": "@hybsearch/hybsearch",
  "private": true,
  "version": "4.0.0-beta.11",
  "description": "Find nonmonophyly in genetic sequences",
  "repository": "hybsearch/hybsearch",
  "homepage": "https://hybsearch.github.io/",
  "main": "main.js",
  "license": "MIT",
  "author": "The Hybsearch Team <hybsearch@stolaf.edu>",
  "scripts": {
    "start": "cd electron && npm start",
    "test": "jest",
    "lint": "eslint --cache 'server/**/*.js' 'server/**/hyb-*' 'electron/**/*.js'",
    "pretty-check": "npm run prettify -- --list-different",
    "prettify": "prettier --write 'server/**/*.js' 'server/**/hyb-*' 'electron/**/*.{js,css}'"
  },
  "prettier": {
    "overrides": [
      {
        "files": "server/**/hyb-*",
        "options": {
          "parser": "babylon"
        }
      }
    ],
    "useTabs": true,
    "semi": false,
    "trailingComma": "es5",
    "singleQuote": true
  },
  "jest": {
    "verbose": true
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "eslint": "^4.19.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-react": "^7.7.0",
    "expect": "^22.4.0",
    "flow-bin": "^0.69.0",
    "jest": "^22.4.2",
    "parcel-bundler": "^1.6.2",
    "prettier": "^1.11.1",
    "pretty-quick": "^1.4.1"
  },
  "dependencies": {
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0"
  }
}
