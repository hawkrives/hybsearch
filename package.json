{
  "name": "hybsearch",
  "private": true,
  "version": "4.0.0-beta.1",
  "description": "Find nonmonophyly in genetic sequences",
  "repository": "hybsearch/hybsearch",
  "homepage": "https://hybsearch.github.io/",
  "main": "main.js",
  "license": "MIT",
  "scripts": {
    "build": "electron-builder --mac --windows --linux",
    "pack": "electron-builder --dir",
    "start": "./hybsearch .",
    "prettier": "prettier --write bin/*.js lib/*.js electron/*.{js,css} scripts/*.js vendor/*.js"
  },
  "build": {
    "appId": "edu.stolaf.bio.hybsearch",
    "productName": "HybSearch"
  },
  "prettier": {
    "useTabs": true,
    "semi": false,
    "trailingComma": "es5",
    "singleQuote": true
  },
  "bundledDependencies": [
    "biojs-io-newick",
    "combinations-generator",
    "d3",
    "dedent",
    "electron-unhandled",
    "execa",
    "file-extension",
    "get-stdin",
    "lodash",
    "loud-rejection",
    "minimist",
    "normalize.css",
    "serialize-error",
    "tempfile",
    "ws"
  ],
  "dependencies": {
    "biojs-io-newick": "1.5.0",
    "combinations-generator": "1.0.1",
    "d3": "3.5.17",
    "dedent": "0.7.0",
    "electron-unhandled": "0.2.0",
    "execa": "0.8.0",
    "get-stdin": "5.0.1",
    "lodash": "4.17.4",
    "loud-rejection": "1.6.0",
    "minimist": "1.2.0",
    "normalize.css": "7.0.0",
    "serialize-error": "2.1.0",
    "tempfile": "2.0.0",
    "wordwrap": "1.0.0",
    "ws": "4.0.0"
  }
}
