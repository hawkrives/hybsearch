{
  "name": "@hybsearch/electron",
  "private": true,
  "version": "4.0.0-beta.3",
  "description": "an electron gui for hybsearch",
  "repository": "hybsearch/hybsearch",
  "homepage": "https://hybsearch.github.io/",
  "license": "MIT",
  "author": "The Hybsearch Team <hybsearch@stolaf.edu>",
  "scripts": {
