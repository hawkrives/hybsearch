{
  "name": "hybsearch-electron",
  "private": true,
  "version": "0.0.0",
  "description": "an electron gui for hybsearch",
  "repository": "hybsearch/hybsearch",
  "homepage": "https://hybsearch.github.io/",
  "license": "MIT",
  "author": "The Hybsearch Team <hybsearch@stolaf.edu>",
  "scripts": {
    "build": "parcel build --detailed-report index.html",
    "pack": "electron-builder --dir",
    "test": "PATH=$PATH:../node_modules/.bin; jest",
    "lint": "cd .. && npm run lint",
    "prettify": "cd .. && npm run prettify",
    "pretty-check": "cd .. && npm run pretty-check",
    "start": "parcel serve ./index.html"
  },
  "dependencies": {
    "d3": "^3.5.17",
    "lodash": "^4.17.5",
    "loud-rejection": "^1.6.0",
    "normalize.css": "^8.0.0",
    "pretty-ms": "^3.1.0"
  },
  "devDependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "parcel-bundler": "^1.7.0"
  }
}
