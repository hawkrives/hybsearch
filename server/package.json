{
  "name": "hybsearch-server",
  "private": true,
  "version": "0.0.0",
  "description": "Find nonmonophyly in genetic sequences",
  "repository": "hybsearch/hybsearch",
  "homepage": "https://hybsearch.github.io/",
  "license": "MIT",
  "author": "The Hybsearch Team <hybsearch@stolaf.edu>",
  "bin": {
    "hyb-ent": "./ent/hyb-ent",
    "hyb-fasta-to-nexus": "./formats/hyb-fasta-to-nexus",
    "hyb-genbank-to-fasta": "./formats/hyb-genbank-to-fasta",
    "hyb-nexus-to-fasta": "./formats/hyb-nexus-to-fasta",
    "hyb-consensus-to-newick": "./newick/hyb-consensus-to-newick",
    "hyb-newick-to-json": "./newick/hyb-newick-to-json",
    "hyb-pipeline": "./pipeline/hyb-pipeline",
    "hyb-clustal": "./wrappers/clustal/hyb-clustal",
    "hyb-mrbayes": "./wrappers/mrbayes/hyb-mrbayes",
    "hyb-seq-gen": "./wrappers/seq-gen/hyb-seq-gen"
  },
  "scripts": {
    "start": "env NODE_ENV=production ./server.js",
    "lint": "cd .. && npm run lint",
    "prettify": "cd .. && npm run prettify",
    "pretty-check": "cd .. && npm run pretty-check",
    "test": "PATH=$PATH:../node_modules/.bin; jest"
  },
  "dependencies": {
    "combinations-generator": "^1.0.1",
    "comma-separated-values": "^3.6.4",
    "dedent": "^0.7.0",
    "execa": "^0.10.0",
    "get-stdin": "^6.0.0",
    "koa": "^2.5.0",
    "koa-compress": "^2.0.0",
    "koa-logger": "^3.2.0",
    "koa-router": "^7.4.0",
    "lodash": "^4.17.5",
    "loud-rejection": "^1.6.0",
    "make-dir": "^1.2.0",
    "minimist": "^1.2.0",
    "serialize-error": "^2.1.0",
    "tempy": "^0.2.1",
    "wordwrap": "^1.0.0",
    "ws": "^5.1.1"
  },
  "optionalDependencies": {
    "bufferutil": "^3.0.4",
    "utf-8-validate": "^4.0.1"
  }
}
